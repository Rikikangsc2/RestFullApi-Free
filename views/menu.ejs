<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NueApi</title>
    <link rel="icon" type="image/png" href="https://telegra.ph/file/e07e0fddc2a2967bd08d6.jpg"/>
    <meta name="description" content="Rest apis WhatsApp bot, Telegram bot, and Discord bot, free, has AI download IG feature and others">
    <meta name="tag" content="free api, nue-api, rest-api, azzapi">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600&display=swap');

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Poppins', sans-serif;
            background-color: #f4f4f4;
            color: #333;
            display: flex;
            flex-direction: column;
            min-height: 100vh;
        }

        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 30px;
        }

        header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            background-color: #3498db;
            color: #fff;
            padding: 20px;
            margin-bottom: 30px;
            border-radius: 10px;
        }

        .logo {
            display: flex;
            align-items: center;
        }

        .logo img {
            width: 40px;
            height: 40px;
            margin-right: 10px;
        }

        .logo h1 {
            font-size: 24px;
            margin: 0;
        }

        .dashboard-link {
            color: #fff;
            text-decoration: none;
            font-size: 18px;
            transition: color 0.3s;
        }

        .dashboard-link:hover {
            color: #ecf0f1;
        }

        .search-container {
            margin-bottom: 30px;
        }

        .search-container input[type="text"] {
            width: 100%;
            padding: 12px;
            border: 1px solid #ccc;
            border-radius: 5px;
            font-size: 16px;
        }

        .menu-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            grid-gap: 30px;
        }

        .menu-card {
            background-color: #fff;
            border-radius: 10px;
            padding: 30px;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
            transition: transform 0.2s ease, box-shadow 0.2s ease;
            text-decoration: none;
            color: inherit;
            cursor: pointer;
        }

        .menu-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
        }

        .menu-card h2 {
            color: #3498db;
            font-size: 20px;
            margin-bottom: 10px;
        }

        .menu-card p {
            font-size: 14px;
            color: #666;
            margin-bottom: 20px;
        }

        .try-button {
            background-color: #3498db;
            color: #fff;
            padding: 10px 20px;
            border: none;
            border-radius: 5px;
            cursor: pointer;
            transition: background-color 0.3s;
        }

        .try-button:hover {
            background-color: #2980b9;
        }

        footer {
            background-color: #333;
            color: #fff;
            text-align: center;
            padding: 10px;
            margin-top: 30px;
            border-radius: 10px;
        }

        footer p {
            font-size: 12px;
        }

        .no-results {
            text-align: center;
            font-size: 16px;
            color: #666;
        }
    </style>
</head>
<body>
    <div class="container">
        <header>
            <div class="logo">
                <img src="https://telegra.ph/file/e07e0fddc2a2967bd08d6.jpg" alt="NueApi Logo">
                <h1>NueApi</h1>
            </div>
            <a href="/docs" class="dashboard-link">Back</a>
        </header>

        <div class="search-container">
            <input type="text" id="search-bar" placeholder="Cari API...">
        </div>

        <main>
            <div class="menu-grid" id="menu-container">

            </div>
        </main>

        <footer>
            <p>Â© 2023 NueApi. All rights reserved.</p>
        </footer>
    </div>

    <script>
        const menuItems = [
            {
                title: 'NueGo AI',
                description: 'AI Lanjutan buatan NueApi di lengkapi search engine. Search engine tidak akan aktif bila hanya mengobrol biasa!.',
                endpoint: '/api/nuego',
                params: [
                    { name: 'user', example: 'iky' },
                    { name: 'q', example: 'Aku mau tanya apa itu ikan' }
                ]
            },
            {
                title: 'Nature Text To Speech',
                description: 'Ubah teks menjadi audio dengan suara alami',
                endpoint: '/api/nature-tts',
                params: [
                    { name: 'text', example: 'Halo dunia!' }
                ]
            },
            {
                title: 'AI Logic',
                description: 'Buat AI Anda sendiri dengan model llama3-70b-8192 dengan sesi chat',
                endpoint: '/api/lgpt',
                params: [
                    { name: 'user', example: 'John Doe' },
                    { name: 'systemPrompt', example: 'Anda adalah asisten AI' },
                    { name: 'text', example: 'Halo!' }
                ]
            },
            {
                title: 'Anime Reaction',
                description: 'Dapatkan reaksi anime',
                endpoint: '/api/anime-reaction',
                params: []
            },
            {
                title: 'Anime Schedule',
                description: 'Jadwal anime berdasarkan hari',
                endpoint: '/api/anime-jadwal',
                params: [
                    { name: 'hari', example: 'Senin' }
                ]
            },
            {
                title: 'Calendar Events',
                description: 'Lihat daftar acara kalender',
                endpoint: '/api/acara',
                params: []
            },
            {
                title: 'ChatGPT',
                description: 'Model GPT-3',
                endpoint: '/api/gpt',
                params: [
                    { name: 'prompt', example: 'Apa itu artificial intelligence?' }
                ]
            },
            {
                title: 'Date Now',
                description: 'Waktu, tanggal, dan acara mendatang',
                endpoint: '/api/date',
                params: [
                    { name: 'zone', example: 'Asia/Jakarta' }
                ]
            },
            {
                title: 'FB & IG Downloader',
                description: 'Download video/foto dari Instagram dan Facebook',
                endpoint: '/api/snapsave',
                params: [
                    { name: 'url', example: 'https://www.instagram.com/p/C12345678/' }
                ]
            },
            {
                title: 'Gemini Web',
                description: 'Gemini web version',
                endpoint: '/api/bard',
                params: [
                    { name: 'text', example: 'Jelaskan tentang alam semesta' }
                ]
            },
            {
                title: 'Gemini Model',
                description: 'Model Gemini AI',
                endpoint: '/api/gemini',
                params: [
                    { name: 'prompt', example: 'Tulis puisi tentang hujan' }
                ]
            },
            {
                title: 'Google Search',
                description: 'Cari di Google',
                endpoint: '/api/google',
                params: [
                    { name: 'limit', example: '3' },
                    { name: 'query', example: 'Javascript' }
                ]
            },
            {
                title: 'Image Search',
                description: 'Cari gambar di internet',
                endpoint: '/api/image',
                params: [
                    { name: 'query', example: 'Kucing lucu' }
                ]
            },
            {
                title: 'Info Gempa (BMKG)',
                description: 'Informasi gempa bumi terbaru dari BMKG',
                endpoint: '/api/gempa',
                params: []
            },
            {
                title: 'NueAI (with Sessions)',
                description: 'Berinteraksi dengan NueAI (Model llama3-70b-8192 dengan sesi chat)',
                endpoint: '/api/sgpt',
                params: [
                    { name: 'user', example: 'gojo' },
                    { name: 'text', example: 'Bagaimana kabarmu?' }
                ]
            },
            {
                title: 'Stable Diffusion',
                description: 'AI yang dapat menghasilkan gambar dari teks',
                endpoint: 'https://nue-api.koyeb.app/sdlist',
                params: []
            },
            {
                title: 'Stable Diffusion XL',
                description: 'AI yang dapat menghasilkan gambar dari teks dengan kualitas lebih tinggi',
                endpoint: 'https://nue-api.koyeb.app/sdxllist',
                params: []
            },
            {
                title: 'Text To Speech',
                description: 'Ubah teks menjadi audio',
                endpoint: '/api/tts',
                params: [
                    { name: 'lang', example: 'id' },
                    { name: 'text', example: 'Halo dunia!' }
                ]
            },
            {
                title: 'TikTok Downloader',
                description: 'Download video, foto, dan musik dari TikTok',
                endpoint: '/api/tt-dl',
                params: [
                    { name: 'url', example: 'https://www.tiktok.com/@username/video/1234567890' }
                ]
            },
            {
                title: 'Upscale Image',
                description: 'Upscale gambar dari URL',
                endpoint: '/api/upscale',
                params: [
                    { name: 'url', example: 'https://example.com/gambar.jpg' }
                ]
            },
            {
                title: 'YouTube Downloader',
                description: 'Download video/audio dari YouTube',
                endpoint: '/api/ytdl',
                params: [
                    { name: 'url', example: 'https://www.youtube.com/watch?v=abcdefghijk' }
                ]
            },
            {
                title: 'YouTube Player',
                description: 'Cari dan putar video/audio YouTube',
                endpoint: '/api/play',
                params: [
                    { name: 'query', example: 'Dj Ya odna' }
                ]
            },
            {
                title: 'YouTube Search',
                description: 'Cari video di YouTube',
                endpoint: '/api/yt-search',
                params: [
                    { name: 'query', example: 'Tutorial cara bernafas' }
                ]
            }
        ];

        const menuContainer = document.getElementById('menu-container');
        const searchBar = document.getElementById('search-bar');

        function renderMenuItems(searchTerm = '') {
            menuContainer.innerHTML = '';

            const filteredMenuItems = menuItems.filter(item => {
                return item.title.toLowerCase().includes(searchTerm.toLowerCase()) ||
                       item.description.toLowerCase().includes(searchTerm.toLowerCase());
            });

            if (filteredMenuItems.length === 0) {
                menuContainer.innerHTML = '<p class="no-results">Tidak ada API yang ditemukan.</p>';
                return;
            }

            // Sort the filtered menu items by title
            filteredMenuItems.sort((a, b) => a.title.localeCompare(b.title));

            filteredMenuItems.forEach(item => {
                const card = document.createElement('div');
                card.classList.add('menu-card');

                card.innerHTML = `
                    <h2>${item.title}</h2>
                    <p>${item.description}</p>
                    <button class="try-button" data-endpoint="${item.endpoint}" data-params='${JSON.stringify(item.params)}'>Coba</button>
                `;

                menuContainer.appendChild(card);
            });

            const tryButtons = document.querySelectorAll('.try-button');
            tryButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const endpoint = button.dataset.endpoint;
                    const params = JSON.parse(button.dataset.params || '[]');

                    let finalUrl = endpoint;

                    if (params.length > 0) {
                        let allParamsProvided = true;
                        finalUrl += '?';

                        params.forEach((param, index) => {
                            let inputValue = prompt(`Masukkan nilai untuk ${param.name}: (${param.example || ''})`);
                            if (inputValue !== null && inputValue.trim() !== "") {
                                finalUrl += `${param.name}=${encodeURIComponent(inputValue)}`;
                                if (index < params.length - 1) {
                                    finalUrl += '&';
                                }
                            } else {
                                allParamsProvided = false;
                            }
                        });

                        if (!allParamsProvided) {
                            alert('Mohon isi semua parameter yang diperlukan.');
                            return;
                        }
                    }

                    window.location.href = finalUrl;
                });
            });
        }

        searchBar.addEventListener('input', () => {
            const searchTerm = searchBar.value;
            renderMenuItems(searchTerm);
        });

        renderMenuItems();
    </script>
</body>
</html>
